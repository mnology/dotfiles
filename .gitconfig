[gui]
	fontui = -family \"Anonymous Pro Minus\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"Source Code Pro for Powerline\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
[user]
	email = ejohanse@jaguarlandrover.com
	name = Erik Johansen
[alias]
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
	ui = true
[diff]
	tool = meld
[branch]
	autosetupmerge = true
[push]
	default = simple
[core]
	packedGitWindowSize = 16m
	packedGitLimit = 128m
[pack]
  windowMemory = 512m
[difftool]
  prompt = false
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	co = checkout
	st = status
	gui = !sh -c /usr/local/Cellar/git/2.8.3/libexec/git-core/git-gui
[core]
  excludesfile = /Users/**USERNAME**/.gitignore_global
[merge]
  tool=meld
[mergetool]
  prompt = false
