[**.js]
indent_style = space
indent_size = 2
max_preserve_newlines = 10
space_in_paren = true
brace_style = end-expand
jslint_happy = true
space_before_conditional = true
wrap_line_length = 0
break_chained_methods = true
good_stuff = true
space_after_anon_function = true
indent_scripts = true

[**.css]
indent_style = space
indent_size = 2

[**.html]
indent_style = space
indent_size = 2
max_char = 100
